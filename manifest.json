{
  "manifest_version": 2,
  "name": "Force Push",
  "description": "git push -f origin master",
  "version": "0.0.1",
  "author": "ahollenbach",
  "browser_action": {
    "default_title": "force PUSH!",
    "default_icon": "icon_gray_32.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "https://github.com/*",
    "contextMenus"
  ]
}